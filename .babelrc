{
    "presets": [
      "next/babel"
    ],
    "plugins": [
      "inline-dotenv",
      [
        "module-resolver", {
          "root": ["."],
          "alias": {
            "styles": "./styles"
          },
          "cwd": "babelrc"
      }],
      [
        "wrap-in-js",
        {
          "extensions": ["css$", "scss$"]
        }
      ]
    ]
  }